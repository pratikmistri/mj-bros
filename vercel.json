{
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)\\.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!src|assets|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
